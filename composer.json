{
    "name": "persata/linkedin-resume-parser",
    "type": "library",
    "description": "Parses LinkedIn PDF resumes into a set of usable entities",
    "keywords": [
        "linkedin",
        "pdf",
        "resume",
        "parser"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "persata",
            "email": "persata@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "smalot/pdfparser": "^0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^0.9.8"
    },
    "autoload": {
        "psr-4": {
            "LinkedInResumeParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LinkedInResumeParser\\Tests\\": "tests/"
        }
    }
}
